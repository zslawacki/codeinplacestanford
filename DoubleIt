def main():
    """
    You should write your code here. 
    """
    repeat_upto_100()
    
def repeat_upto_100():
    curr_value = int(input("Enter a number: "))
    double_value = curr_value * 2
    print (double_value)
    while double_value <= 100:
        double_value = double_value * 2
        print (double_value)
    
    

if __name__ == '__main__':
    main()
